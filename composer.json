{
    "name": "plattry/http",
    "description": "The plattry http package",
    "keywords": [
        "plattry",
        "http"
    ],
    "type": "library",
    "license": "GPL-3.0",
    "autoload": {
        "psr-4": {
            "Plattry\\Http\\": "src/"
        },
        "exclude-from-classmap": [
            "/test"
        ]
